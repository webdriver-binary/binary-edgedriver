{
    "name": "webdriver-binary/binary-edgedriver",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Utility for downloading Microsoft Edge Driver binary with browser version detection",
    "require": {
        "php": ">=7.2",
        "composer-plugin-api": "^1.0 | ^2.0",
        "webdriver-binary/webdriver-binary-downloader": "^3.0.0"
    },
    "replace": {
        "vaimo/binary-edgedriver": "*",
        "lanfest/binary-edgedriver": "*"
    },
    "support": {
        "source": "https://github.com/webdriver-binary/binary-edgedriver",
        "docs": "https://github.com/webdriver-binary/binary-edgedriver",
        "issues": "https://github.com/webdriver-binary/binary-edgedriver/issues"
    },
    "keywords": [
        "edgedriver",
        "microsoft edge automation",
        "testing",
        "composer plugin",
        "composer extension",
        "test-automation",
        "acceptance testing",
        "browser testing",
        "composer plugin",
        "composer extension",
        "browser driver",
        "webdriver",
        "downloader",
        "binary downloader",
        "binary"
    ],
    "autoload": {
        "psr-4": {
            "WebdriverBinary\\EdgeDriver\\": "src"
        }
    },
    "extra": {
        "class": "WebdriverBinary\\EdgeDriver\\Plugin"
    }
}
